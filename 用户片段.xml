{
    //============ Items =============

    //-------- Container ---------
    "< Item.UIView >": {
        "prefix": "<View",
        "body": [
            "<View ${1:propertys=\"value\"}>",
            "\t${2:<Items>}",
            "</View>",
        ],
        "description": "container View"
    },

    //-------- Single Item ----------
    "< Item.UILabel >": {
        "prefix": "<Label",
        "body": [
            "<Label ${1:property} />",
        ],
        "description": "Item Label"
    },
    "< Item.UITextField >": {
        "prefix": "<TextField",
        "body": [
            "<TextField ${1:property} />",
        ],
        "description": "Item TextField"
    },
    "< Item.UIImageView >": {
        "prefix": "<Image",
        "body": [
            "<Image ${1:property} />",
        ],
        "description": "Item Image"
    },
    "< Item.UIButton >": {
        "prefix": "<Button",
        "body": [
            "<Button ${1:property} />",
        ],
        "description": "Item Button"
    },


